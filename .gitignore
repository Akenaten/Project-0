Project-0/*.booproj
Project-0/*.svd
Project-0/*.pdb
Project-0/*.mdb
Project-0/*.opendb
Project-0/*.VC.db

# Unity3D generated meta files
Project-0/*.pidb.meta
Project-0/*.pdb.meta
Project-0/*.mdb.meta

# Unity3D generated file on crash reports
Project-0/sysinfo.txt

# Builds
Project-0/*.apk
Project-0/*.aab
Project-0/*.unitypackage
Project-0/*.app

# Crashlytics generated file
crashlytics-build.properties

# Packed Addressables
**/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
**/[Aa]ssets/[Ss]treamingAssets/aa.meta
